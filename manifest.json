{
  "manifest_version": 3,
  "name": "Nathan is great",
  "version": "2.5",
  "description": "Does lots of things, some of them useful.",
  "permissions": [
    "tabs",
    "downloads"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.vantage-modules.co.uk/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}